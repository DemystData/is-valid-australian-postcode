{
  "name": "is-valid-australian-postcode",
  "version": "1.0.0",
  "description": "Check if a number is a valid Australian postcode",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "source": "src/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build-esm && yarn build-umd",
    "build-esm": "BABEL_ENV=production-esm babel src -d dist/esm --ignore **/*.spec.js",
    "build-umd": "BABEL_ENV=production-umd babel src -d dist/umd --ignore **/*.spec.js",
    "clean": "rimraf dist",
    "jest": "jest",
    "lint": "eslint src/ --ext .jsx,.js",
    "prepublishOnly": "yarn clean && yarn build",
    "test": "yarn lint && yarn jest"
  },
  "keywords": [
    "validation",
    "australia",
    "postcode",
    "postcode-validation"
  ],
  "author": {
    "name": "Wojciech Maj",
    "email": "kontakt@wojtekmaj.pl"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.0",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.0",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "jest": "^24.8.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "src/"
  ],
  "repository": "https://github.com/DemystData/is-valid-australian-postcode.git"
}
